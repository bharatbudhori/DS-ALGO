#include <iostream>
using namespace std;


int main()
{
    int A[] = {33,33,26,7,19,23,8,43};
    float sum=0;
    float s=0;
    for(int i=0;i<8;i++){
        s+=A[i];
        
    }
    cout<<s<<endl;
    
    for(int i=0;i<8;i++){
       sum+=A[i];
       float x=(s-A[i+1])/2;
       
       if(x==sum){
           cout<<i+2<<endl;
           break;
       }
    }
    
    cout<<"ops";

    return 0;
}
