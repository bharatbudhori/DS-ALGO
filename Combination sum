#include<iostream>
#include<bits/stdc++.h>

using namespace std;

void combinationSum(vector<int>& arr, int target, string ans){
    if(target == 0){
        cout<<ans;
        cout<<endl;
        return;
    }
    for(int i= 0; i<arr.size() && arr[i]<=target ;i++){
        char ch='0' + arr[i];
        
        
        combinationSum(arr, target-arr[i], ans+ch);
        
    }
    
}


int main()
{
    
    vector<int> arr{2,3,6,7};
    vector<int> ans;
    
    combinationSum(arr, 7, "");
   
    

    return 0;
}





