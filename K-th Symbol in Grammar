#include<iostream>
#include<bits/stdc++.h>

using namespace std;

string kthGrammar(string ans, int n, int x){
    if(x==n){
        return ans;
    }
    if(ans.length() == 0){
        return kthGrammar("0", n+1, x);
    }
    else{
        string str = "";
        for(int i=0; i<ans.size(); i++){
            if(ans[i]==0){
                str.append("01");
            }
            else{
                str.append("10");
            }
        }
        return kthGrammar(str, n+1, x);
    }
    
}



int main()
{
    string str=kthGrammar("", 1, 4);
    cout<<str;
    
    
    

    return 0;
}


